{
  "cli": {
    "version": ">= 16.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "music-development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_TOPIC": "music"
      }
    },
    "music-preview": {
      "distribution": "internal",
      "env": {
        "APP_TOPIC": "music"
      }
    },
    "music-production": {
      "autoIncrement": true,
      "env": {
        "APP_TOPIC": "music"
      }
    },
    "nineties-development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_TOPIC": "nineties"
      }
    },
    "nineties-preview": {
      "distribution": "internal",
      "env": {
        "APP_TOPIC": "nineties"
      }
    },
    "nineties-production": {
      "autoIncrement": true,
      "env": {
        "APP_TOPIC": "nineties"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {},
    "music-production": {},
    "nineties-production": {}
  }
}
